<!-- Disabling MTOM for tooling org.wso2.carbon.policybuilder.ui.internal.services. If MTOM is enabled globally
this will case an adverse effect on these org.wso2.carbon.policybuilder.ui.internal.services when calling
through the AJAX admin console.
-->
<service name="UploaderService" >
    <transports>
        <transport>https</transport>
        <transport>http</transport>
    </transports>


    <messageReceivers>
        <messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-out"
                         class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
    </messageReceivers>
    <parameter locked="false" name="ServiceClass">org.wso2.carbon.policybuilder.ui.UploaderService</parameter>
    <parameter name="hiddenService" locked="true">true</parameter>


</service>

