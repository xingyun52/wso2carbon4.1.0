<serviceGroup>
    <service name="SynapseArtifactUploaderAdmin" scope="transportsession">
        <schema schemaNamespace="http://org.apache.axis2/xsd" elementFormDefaultQualified="true"/>
        <transports>
            <transport>https</transport>
        </transports>
        <description>
            Admin service for Uploading Synapse Artifacts
        </description>
        <parameter name="ServiceClass">org.wso2.carbon.mediation.artifactuploader.SynapseArtifactUploaderAdmin</parameter>
        <parameter name="enableMTOM" locked="false">true</parameter>
    </service>

    <parameter name="adminService" locked="true">true</parameter>
    <parameter name="hiddenService" locked="true">true</parameter>
    <parameter name="AuthorizationAction" locked="false">/permission/admin/manage/modify/webapp
    </parameter>

</serviceGroup>