<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <parent>
        <groupId>org.wso2.carbon</groupId>
        <artifactId>carbon-components</artifactId>
        <version>4.1.0</version>
    </parent>

    <modelVersion>4.0.0</modelVersion>
    <groupId>org.wso2.carbon</groupId>
    <artifactId>issue-tracker</artifactId>
    <packaging>pom</packaging>
    <name>WSO2 Carbon - Issue Tracker Module</name>
    <url>http://wso2.org</url>

    <modules>
        <module>org.wso2.carbon.issue.tracker.adapter/4.1.0</module>
        <module>org.wso2.carbon.jira.reporting/4.1.0</module>
        <module>org.wso2.carbon.issue.tracker.core/4.1.0</module>
        <module>org.wso2.carbon.issue.tracker.ui/4.1.0</module>
        <module>org.wso2.carbon.issue.tracker.mgt/4.1.0</module>
    </modules>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>org.wso2.carbon</groupId>
                <artifactId>org.wso2.carbon.core</artifactId>
                <version>${wso2carbon.version.issue-tracker}</version>
            </dependency>
            <dependency>
                <groupId>org.apache.axis2.wso2</groupId>
                <artifactId>axis2</artifactId>
                <version>${orbit.version.axis2.issue-tracker}</version>
            </dependency>
            <dependency>
                <groupId>org.wso2.carbon</groupId>
                <artifactId>org.wso2.carbon.ui</artifactId>
                <version>${wso2carbon.version.issue-tracker}</version>
            </dependency>
            <dependency>
                <groupId>junit</groupId>
                <artifactId>junit</artifactId>
                <version>${junit.version.issue-tracker}</version>
                <scope>test</scope>
            </dependency>
        </dependencies>
    </dependencyManagement>

    <properties>
        <wso2carbon.version.issue-tracker>${wso2carbon.version}</wso2carbon.version.issue-tracker>
        <orbit.version.axis2.issue-tracker>${orbit.version.axis2}</orbit.version.axis2.issue-tracker>
        <axis2.osgi.version.range.issue-tracker>${axis2.osgi.version.range}
        </axis2.osgi.version.range.issue-tracker>
        <axiom.osgi.version.range.issue-tracker>${axiom.osgi.version.range}
        </axiom.osgi.version.range.issue-tracker>
        <junit.version.issue-tracker>${junit.version}</junit.version.issue-tracker>
    </properties>

</project>