<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">

    <description>
        Carbon Data Source lookup Example.
    </description>
    <display-name>Carbon Data Source lookup Example</display-name>

    <servlet>
        <servlet-name>DataSourceAccessServlet</servlet-name>
        <servlet-class>org.wso2.appserver.sample.carbondatasource.servlet.CarbonDSLookupServlet
        </servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DataSourceAccessServlet</servlet-name>
        <url-pattern>/dslookup</url-pattern>
    </servlet-mapping>


</web-app>
