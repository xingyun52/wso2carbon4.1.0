    <sequence name="main">
        <in>
            <class name="org.wso2.carbon.mediator.clazz.test.classes.SampleClassWithProperties">
                <axis2ns5:property xmlns:axis2ns5="http://ws.apache.org/ns/synapse" name="symbol" value="MSFT"/>
